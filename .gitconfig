 [push]
	default = upstream
 [core]
     trustctime = false
     editor = vim
     filemode = false
     autocrlf = false
     safecrlf = false
#     excludesfile = ~/.gitignore_global
 [color]
     ui = true
 [credential]
     helper = cache --timeout=3600
 [merge]
     tool = vimdiff
 [mergetool]
     keeptemporaries = false
     keepbackups = false
     prompt = false
     trustexitcode = false
 [alias]
     last = log -1 --stat
     cp = cherry-pick
     co = checkout
     cl = clone
     ci = commit
    cif = commit . -m 'fix'
     st = status 
     br = branch -vv
     unstage = reset HEAD --
     dc = diff --cached
     lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %Cblue<%an>%Creset' --abbrev-commit --date=relative --all
     pl = pull
     ps = push
 [user]
     name = 
     email =
 [pager]
     branch = false

